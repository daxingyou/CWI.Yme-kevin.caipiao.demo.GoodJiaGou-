.fixed_h{
	position: fixed;
    width: 100%;
    max-width: 640px;
    top: 0;
    z-index: 1;
}
.orderSerch .statusBox{
	display: flex;
	border-bottom: 1px solid #eaeaea;
	position: relative;
    background-color: #fff;
	display: -webkit-flex;
}
.orderSerch .statusBox>div{
	width:33%;
	height: 44px;
	text-align: center;
	
}
.orderSerch .statusBox>div p{
	display: block;
	padding:2px 0;
	margin: 10px auto;
}
.bor-r-l{
	border-right: 1px solid #eaeaea;border-left: 1px solid #eaeaea;
}
.orderSerch .statusBox>div p span{
	position: relative;
	padding-right: 16px;
}
.orderSerch .statusBox>div p span:after{
	content: '';
	border:6px solid transparent;
	display: inline-block;
	position: absolute;
	right: 0;
	top:50%;
	margin-top: -3px;
	border-top-color: #aaa;
}
.orderSerch .statusBox>div.active p span:after{
	transform: rotate(180deg);
	border-top-color: rgb(49,199,160);
	margin-top: -9px;
}
.orderSerch .statusBox>div.active p{
	color:rgb(49,199,160);
}
.orderSerch .statusBox>div ul{
	position: absolute;
	width:100%;
	left:0;
	top:45px;
	z-index: 1111111;
	background-color: #fff;
}
.order .tip{
	padding:4px 10px;
	background-color: #F6F5FA;
	font-size: 13px;
	color:#999
}
.orderSerch .statusBox>div ul li{
	border-bottom: 1px solid #eaeaea;
	padding:10px 20px;
	text-align: left;
	position: relative;
}
.orderSerch .statusBox>div ul li.active{
	color: rgb(49,199,160);
}
.orderSerch .statusBox>div ul li.active:after{
	content: '';
	position: absolute;
	right: 20px;
	width:8px;
	height: 16px;
	transform: rotate(45deg);
	border-width:2px;
    border-style: solid;
    border-color:rgb(49,199,160);
    border-top: 0;
    border-left: 0;
}
