}
.opt input{
	position: absolute;
    display: none;
    left: 0;
}
.opt input+ label{
	display: block;
	height: 20px;
    padding-left: 22px;
    position: relative;
    cursor: pointer;
    moz-user-select: -moz-none;
    -moz-user-select: none;
    -o-user-select: none;
    -khtml-user-select: none;
    -webkit-user-select: none;
    -ms-user-select: none;
    user-select: none;
}
.opt input+ label:before{
	content: '';
    width: 16px;
    height: 16px;
    position: absolute;
    left: 0;
    top:3px;
    border-radius: 3px;
    border: 1px solid #31c7a0;
    background-color: #fff;
}
.opt input+ label:after{
	top: 4px;
    left: 5px;
    box-sizing: border-box;
    width: 6px;
    height: 10px;
    transform: rotate(45deg) scale(0);
    border-width:2px;
    border-style: solid;
    border-color: #fff;
    border-top: 0;
    border-left: 0;
    position: absolute;
    transition: all .3s;
    content: '';
}
.opt input:checked+ label:after{
	transform: rotate(45deg) scale(1);
}
.opt input:checked+ label:before {
    border: #31c7a0;
    background: #31c7a0;
}
