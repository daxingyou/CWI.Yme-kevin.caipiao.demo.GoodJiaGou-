.KaMo-refresh-content {
	min-height: 99%;
	bottom: 0;
	padding-bottom: 60px;
    position: relative;
    -webkit-overflow-scrolling: touch;
    -webkit-transform: translate3d(0, 0, 0);
            transform: translate3d(0, 0, 0);
            transition: transform 0.8s;
}

@media (max-width: 360px) {
  .pd-160{
		padding-top: 3em !important;
	}
}
.KaMo-refresh-load {
    width: 100%;
    height: 30px;
    position: absolute;
    top: -3em;
    left: 0;
}
.KaMo-refresh-pull-arrow {
    width: 100%;
