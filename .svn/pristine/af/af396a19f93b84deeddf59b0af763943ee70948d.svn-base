
namespace Yme.Mcp.Model.Waimai.Meituan
{
    /// <summary>
    /// 美团门店信息
    /// </summary>
    public class MeituanShopModel
    {
        /// <summary>
        /// erp方门店id 最大长度100
        /// </summary>
        public string ePoiId { get; set; }

        /// <summary>
        /// 门店名称
        /// </summary>
        public string name { get; set; }

        /// <summary>
        /// 门店公告信息[2000字以内]
        /// </summary>
        public string noticeInfo { get; set; }

        /// <summary>
        /// 门店地址
        /// </summary>
        public string address { get; set; }

        /// <summary>
        /// 美团品类名称
        /// </summary>
        public string tagName { get; set; }

        /// <summary>
        /// 门店经度
        /// </summary>
        public string latitude { get; set; }

        /// <summary>
        /// 门店纬度
        /// </summary>
        public string longitude { get; set; }

        /// <summary>
        /// 门店电话
        /// </summary>
        public string phone { get; set; }

        /// <summary>
        /// 门店图片url
        /// </summary>
        public string pictureUrl { get; set; }

        /// <summary>
        /// 是否在线【1-上线 0-下线】
        /// </summary>
        public int isOnline { get; set; }

        /// <summary>
        /// 是否营业【1-可配送,3-休息中,0-未上线】
        /// </summary>
        public int isOpen { get; set; }

        /// <summary>
        /// 营业时间【06:00-09:00,15:30-22:00】
        /// </summary>
        public string openTime { get; set; }

        /// <summary>
        /// 配送费
        /// </summary>
        public float shippingFee { get; set; }

        /// <summary>
        /// 门店是否支持发票【1-可开发票 0-不支持】
        /// </summary>
        public int invoiceSupport { get; set; }

        /// <summary>
        /// 门店支持开发票的最小订单价
        /// </summary>
        public int invoiceMinPrice { get; set; }

        /// <summary>
        /// 发票相关说明
        /// </summary>
        public string invoiceDescription { get; set; }

        /// <summary>
        /// 是否支持营业时间范围外预下单【0-不支持,1 表示支持】
        /// </summary>
        public int preBook { get; set; }

        /// <summary>
        /// 预下单最小日期【0-从当天算起】
        /// </summary>
        public int preBookMinDays { get; set; }

        /// <summary>
        /// 预下单最大日期【2-后天, 预下单支持从最小日期到最大日期的连续日期。例如：preBookMinDays =0，preBookMaxDays=2，表示支持当天，明天，后天的预下单】
        /// </summary>
        public int preBookMaxDays { get; set; }

        /// <summary>
        /// 是否支持营业时间内预下单【0-不支持,1 表示支持】
        /// </summary>
        public int timeSelect { get; set; }
    }
}
