
namespace Yme.Mcp.Model.Waimai.Meituan
{
    /// <summary>
    /// 美图订单扩展信息
    /// </summary>
    public class MeituanOrderExtrasModel
    {
        /// <summary>
        /// 优惠金额中美团承担的部分
        /// </summary>
        public float mt_charge { get; set; }

        /// <summary>
        /// 优惠金额中商家承担的部分
        /// </summary>
        public float poi_charge { get; set; }

        /// <summary>
        /// 活动优惠金额，是美团承担活动费用和商户承担活动费用的总和
        /// </summary>
        public float reduce_fee { get; set; }

        /// <summary>
        /// 优惠说明（如：满10元减2.5元）
        /// </summary>
        public string remark { get; set; }

        /// <summary>
        /// 活动类型
        /// </summary>
        public int type { get; set; }

        /// <summary>
        /// 
        /// </summary>
        public string act_detail_id { get; set; }

        /// <summary>
        /// 
        /// </summary>
        public long avg_send_time { get; set; }
    }
}
