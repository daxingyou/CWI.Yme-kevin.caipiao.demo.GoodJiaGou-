/*
UI 颜色值 
 */
* {
  /*// box-sizing: content-box;*/
  box-sizing: border-box;
}
body,
p,
h1,
h2,
h3,
h4,
h5,
ul,
li,
dl {
  padding: 0;
  margin: 0;
}
html {
  font-size: 16px;
}
body {
  background-color: #F6F5FA;
  font-size: 16px;
  line-height: 1.6;
  color: #000;
  font-family: "Droid Sans Fallback", "PingFang Regular";
  -webkit-tap-highlight-color:transparent;
}
@media (min-width: 640px) {
  body {
	width: 640px;
	margin: 0 auto;
  }
}
@media (max-width: 375px) {
  html {
  font-size: 14px;
  }
  body {
  font-size: 14px;
  }
}
@media (max-width: 360px) {
  html {
	font-size: 14px;
  }
  body {
	font-size: 12px;
  }
}
a {
  color: #000;
  text-decoration: none;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
a:active {
  outline: none;
  /*background: rgba(0,0,0,0);*/
}
a:focus {
  outline: none;
}
img {
  width: 100%;
  border: none;
}
i {
  font-style: normal;
}
li {
  list-style-type: none;
}
h3 {
  font-weight: 500;
}
textarea {
  resize: none;
}
/**/
.horizontal {
  margin: 0 auto;
}
/*图标*/
.icon {
  display: inline-block;
}

.icon-right:after {
  content: "";
  display: inline-block;
  width: 0.6rem;
  height: 0.6rem;
  border-right: 1px solid #bababa;
  border-bottom: 1px solid #bababa;
  -webkit-transform: rotate(-45deg);
  transform: rotate(-45deg);
}
.icon-left:after {
  content: "";
  display: inline-block;
  width: 0.6em;
  height: 0.6em;
  border-left: 2px solid #fff;
  border-bottom: 2px solid #fff;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}
.icon-right,
.icon-left,
.icon-down,
.icon-up {
  width: 1em;
  height: 1em;
}
.icon-down:after {
  content: "";
  display: inline-block;
  width: 0.6em;
  height: 0.6em;
  border-right: 1px solid #bababa;
  border-bottom: 1px solid #bababa;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
  position: relative;
  top: -0.15em;
}
.icon-up:after {
  content: "";
  display: inline-block;
  width: 0.6em;
  height: 0.6em;
  border-right: 1px solid #bababa;
  border-bottom: 1px solid #bababa;
  -webkit-transform: rotate(-135deg);
  transform: rotate(-135deg);
  position: relative;
  top: 0.15em;
  left: 5px;
}

.icon-text{
  text-align: center;
  color: #fff;
  border-radius: 2px;
  margin-right: 10px;
  line-height: 1;
  padding: 2px;
}
.icon-text-circle,
.icon-circle-right,
.icon-circle-down{
  text-align: center;
  color: #fff;
  border-radius: 100%;
  margin-right: 5px;
  line-height: 1;
  width: 1em;
  height: 1em;
  box-sizing: content-box;
  padding: 5px;
}
.icon-circle-right,
.icon-circle-down{
  padding: 2px;
  background-color: #ccc;
  margin-right: 0;
}
.icon-circle-right:after{
  content: "";
  display: inline-block;
  line-height: 1;
  width: 0.5em;
  height: 0.5em;
  border-right: 1px solid #fff;
  border-bottom: 1px solid #fff;
  -webkit-transform: rotate(-45deg);
  transform: rotate(-45deg);
  position: relative;
  top: -1px;
  left: -1px;
}
.icon-circle-down:after{
  content: "";
  display: inline-block;
  line-height: 1;
  width: 0.5em;
  height: 0.5em;
  border-right: 1px solid #fff;
  border-bottom: 1px solid #fff;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
  position: relative;
  top: -3px;
  left: -1px;
}
